PIPELINE_PROFILE=1
PIPELINE_PROFILE_JSONL=.\\profile.jsonl

# Limits
# MAX_IMAGES: how many samples to download/process this run. Use -1 for unlimited.
MAX_IMAGES=

# Prefetch buffer
# MAX_CANDIDATES: downloader prefetch buffer target (how many images to keep queued for inference).
# If you set DOWNLOAD_QUEUE_MAX explicitly, it overrides MAX_CANDIDATES.
MAX_CANDIDATES=8

SHARP_DEVICE=cuda

HF_UPLOAD=1
HF_UPLOAD_BATCH_SIZE=8
HF_UPLOAD_BATCH_WAIT_MS=500

SPZ_EXPORT=1
SPZ_TIMEOUT_SECS_CUDA=300
SPZ_TIMEOUT_SECS_CPU=900

SHARP_PREDICT_TIMEOUT_SECS_CUDA=600
SHARP_PREDICT_TIMEOUT_SECS_CPU=3600

GSPLAT_UPLOAD=1
GSPLAT_TOTAL_TIMEOUT_SECS=900
GSPLAT_CHUNK_SIZE_MB=16
GSPLAT_TRPC_CONNECT_TIMEOUT_SECS=20
GSPLAT_TRPC_TIMEOUT_SECS=180

# Secrets go to .env.local (gitignored)
# UNSPLASH_ACCESS_KEY=...
# HF_TOKEN=...
# Optional
HF_REPO_ID=eatmorefruit/sharp-ply-share
